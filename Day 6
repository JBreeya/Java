INHERITANCE
keyword - extends
1. class vehicle
{
  int tyres;
  String fuel;
  vehicle()
  {
    tyres = 4;
    fuel = "Diesel"
  }
  void show()
  {
    System.out.println("The number of tyres = " +tyres)
    System.out.println("The fuel = " +fuel)
  }
}

class car extends vehicle
{ 
  String colour;
  int noofseats;
  car()
  {
  void display()
  {
    System.out.println("The colour of car = " +colour);
    System.ou.println("The no. of seats = " +noofseats);
  }
}

class inheritval
{
  public static void(String args[])
  {
    car c = new car();
    c.display();
    c.show();
  }
}

STATIC
2. class data
{
  static void display (int x)
  {
    System.out.println("The value of x = " +x);
  }
  public static void main(String args[])
  {
    display(10);
  }
}

COMMAND LINE ARGUMENT
3. class command
{
  public static void main(String args()]
  {
    for(int i; i<args.length; i++)
    System.out.println(args[i] = " ");
  }
}

THIS OPERATOR
4. class current
{
  int x, y;
  void value (int x , int y)
  {
    this.x = x;
    this.y = y;
  }
  void display ()
  {
    System.out.println("X = " +x)
    System.out.println("Y = " +y)
  }
}

class thisdemo
{
  public static void main(String args[])
  {
    current c = new current();
    c.value(10, 15);
    c.display();
  }
}
